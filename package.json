{
  "name": "hex",
  "version": "0.0.0",
  "description": "Custom Hex Color Role",
  "main": "src/structures/Client.js",
  "scripts": {
    "start": "node .",
    "test": "nodemon .",
    "debug": "node --nolazy --inspect-brk=9229 ."
  },
  "author": {
    "name": "Shayne Hartford (ShayBox)",
    "email": "ShayneEHartford@Gmail.Com"
  },
  "dependencies": {
    "discord.js": "github:discordjs/discord.js",
    "klasa": "github:dirigeants/klasa",
    "os": "^0.1.1",
    "os-toolbox": "^0.2.9",
    "randomcolor": "^0.5.3",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "discord.js-typings": "github:discordjs/discord.js-typings",
    "eslint": "^4.18.1",
    "eslint-config-lloti": "lloti/eslint-config-lloti"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shays-Bots/Hex.git"
  },
  "license": "MIT",
  "eslintConfig": {
    "extends": "eslint-config-lloti",
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module"
    },
    "rules": {
      "one-var": [
        "error",
        {
          "var": "consecutive",
          "let": "consecutive",
          "const": "consecutive",
          "separateRequires": true
        }
      ],
      "quotes": [
        "error",
        "backtick"
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  }
}
